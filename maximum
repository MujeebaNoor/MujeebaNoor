const num = [5,2,8,1,7];
let vaalue =findmin(num)
console.log(vaalue)
function findmax(arr){
    if(arr.length == 0){
        console.log("array is empty")
    }
    let max = arr[0]
    for(let i=0; i<arr.length; i++){
        if(arr[i]>max){
            max = arr[i]
        }
    }
    return max;
}
